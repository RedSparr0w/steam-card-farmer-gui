{
  "name": "scf",
  "description": "Automatically farms card drops for Steam games you own with remaining drops",
  "version": "2.0.0",
  "preferGlobal": true,
  "dependencies": {
    "cheerio": "^0.18.0",
    "steam-user": "^3.3.0",
    "steam-client": "^1.1.1",
    "request": "^2.51.0"
  },
  "license": "MIT",
  "scripts": {
    "start" : "nwbuild -v v0.12.3 -r \"./\"",
	"preinstall": "npm list nw-builder -g || npm install nw-builder -g",
	"build": "nwbuild -v v0.12.3 -o \"../builds\" -p win64,win32 --winIco \"./logo.ico\" \"./\""
  },
  "main": "index.html", 
  "window": {
    "frame": false,
    "toolbar": false,
	"width": 350,
	"height": 450,
	"resizable": false,
    "position": "center",
    "min_width": 350,
    "min_height": 450,
    "max_width": 350
  }
}
